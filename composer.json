{
    "name": "watson/sitemap",
    "description": "Generate Google Sitemaps in Laravel 4/5",
    "keywords": [
        "laravel",
        "sitemaps"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@studious.com.au"
        }
    ],
    "require": {
        "php": "~8.0",
        "illuminate/console": "~8.0",
        "illuminate/database": "~8.0",
        "illuminate/http": "~8.0",
        "illuminate/support": "~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/Watson"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Watson\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Sitemap\\SitemapServiceProvider"
            ]
        }
    }
}
